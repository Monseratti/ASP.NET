@{
}
@model List<User>

@if (Model != null)
{
   <table>
       <thead>
           <tr>
               <th>Name</th>
               <th>Email</th>
               <th>Phone</th>
           </tr>
       </thead>
       <tbody>
       @foreach(var User in Model)
       {
           <tr>
               <td>@User.UserName</td>
               <td>@User.Email</td>
               <td>@User.PhoneNumber</td>
           </tr>
       }
       </tbody>
   </table>
  
}